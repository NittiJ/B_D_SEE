BDC_Data %>%
  filter(str_detect(Bdc_Program_DESCRIPTION, "FERTILITY", ignore_case = TRUE)) %>%
  count(Designation_TYPE, sort = TRUE)


BDC_Data %>%
  filter(str_detect(Bdc_Program_DESCRIPTION, "FERTILITY", ignore_case = TRUE)) %>%
  distinct(PROV_MSTR_LOC_ID, PROV_NPI) %>%
  nrow()


BDC_Data %>%
  filter(str_detect(Bdc_Program_DESCRIPTION, "FERTILITY", ignore_case = TRUE)) %>%
  distinct(Bdc_Program_DESCRIPTION)


BDC_Data %>%
  filter(str_detect(Bdc_Program_DESCRIPTION, "FERTILITY", ignore_case = TRUE)) %>%
  distinct(PROV_MSTR_LOC_ID, PROV_NPI, Designation_TYPE) %>%
  count(Designation_TYPE)
