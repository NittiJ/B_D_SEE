BDC_Data %>%
  filter(Bdc_Program_DESCRIPTION == "FERTILITY CARE") %>%
  nrow()

BDC_Data %>%
  filter(Bdc_Program_DESCRIPTION == "FERTILITY CARE") %>%
  distinct(PROV_MSTR_LOC_ID) %>%
  nrow()


BDC_Data %>%
  filter(Bdc_Program_DESCRIPTION == "FERTILITY CARE") %>%
  distinct(PROV_MSTR_LOC_ID, Designation_TYPE) %>%
  count(Designation_TYPE)


filter(
  Bdc_Program_DESCRIPTION == "FERTILITY CARE",
  Designation_TYPE %in% c("BDC", "BDC+")
)


filter(
  Bdc_Program_DESCRIPTION == "FERTILITY CARE",
  is.na(Designation_TYPE) | Designation_TYPE %in% c("BDC", "BDC+")
)





BDC_Data %>%
  filter(
    Bdc_Program_DESCRIPTION == "FERTILITY CARE",
    !is.na(RECOGNTN_ENTITY_CD),
    !is.na(Recogntn_Value_CD)
  ) %>%
  distinct(PROV_MSTR_LOC_ID, PROV_NPI, Designation_TYPE) %>%
  count(Designation_TYPE)
